<h1>Orders</h1>

<table class="table table-dark table-hover">
    <tr>
        <th>Order id</th>
        <th>Status</th>
        <th>Ordered Date</th>
        <th>Edit Order</th>
    </tr>
    <!-- loop -->
    <% orders.forEach(function (order,index) { %>
        <tr>
            <td>
                
            </td>
            <td><%= order.status %></td>
            <td><%= order.ceartedAt %></td>
            <td>
                <a href="order/detail?id=<%= order._id %>" class="btn btn-outline-dark">
                    Edit
                </a>
            </td>
        </tr>
    <% }) %>
    
</table>